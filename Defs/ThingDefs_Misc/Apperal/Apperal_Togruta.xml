<?xml version="1.0" encoding="utf-8" ?>
<Defs>

    <ThingDef ParentName="ApparelMakeableBase">
        <defName>Apparel_JediPadawanOutfit</defName>
        <label>Jedi Padawan Outfit</label>
        <description>The Jedi Padawan.</description>
        <techLevel>Industrial</techLevel>
        <recipeMaker>
            <recipeUsers>
				<li>ElectricTailoringBench</li>
				<li>HandTailoringBench</li>
			</recipeUsers>
        </recipeMaker>
        <graphicData>
            <texPath>Things/Item/Equipment/Apparel/JediPadawanOutfit/TGJediPadawanDisplay</texPath>
            <graphicClass>Graphic_Single</graphicClass>
            <shaderType>CutoutComplex</shaderType>
        </graphicData>
        <costStuffCount>80</costStuffCount>
        <stuffCategories>
            <li>Fabric</li>
            <li>Leathery</li>
        </stuffCategories>
        <costList>
            <Steel>5</Steel>
        </costList>
        <thingCategories>
            <li>Apparel</li>
        </thingCategories>
        <statBases>
            <Mass>1.7</Mass>
			<MaxHitPoints>160</MaxHitPoints>
            <WorkToMake>7000</WorkToMake>
			<StuffEffectMultiplierArmor>0.3</StuffEffectMultiplierArmor>
			<StuffEffectMultiplierInsulation_Cold>0.8</StuffEffectMultiplierInsulation_Cold>
			<StuffEffectMultiplierInsulation_Heat>0.3</StuffEffectMultiplierInsulation_Heat>
            <EquipDelay>1.5</EquipDelay>
        </statBases>
        <equippedStatOffsets>
            <SocialImpact>0.05</SocialImpact>
            <NegotiationAbility>0.05</NegotiationAbility>
            <TradePriceImprovement>0.05</TradePriceImprovement>
            <MeleeHitChance>0.05</MeleeHitChance>
            <MeleeDodgeChance>0.05</MeleeDodgeChance>
        </equippedStatOffsets>
        <apparel>
            <bodyPartGroups>            
				<li>Torso</li>
                <li>Neck</li>
				<li>Shoulders</li>
				<li>Arms</li>
            </bodyPartGroups>
            <wornGraphicPath>Things/Item/Equipment/Apparel/JediPadawanOutfit/TGJediPadawan</wornGraphicPath>
            <layers>
                <li>OnSkin</li>
            </layers>
            <tags>
                <li>Outlander</li>
                <li>Spacer</li>
                <li>JediPadawanOutfit</li>
            </tags>
            <defaultOutfitTags>
                <li>Worker</li>
                <li>Soldier</li>
            </defaultOutfitTags>
        </apparel>
    </ThingDef>

